@startuml
    skinparam backgroundColor #ffffff
    skinparam ArrowColor #6b5336
    skinparam ActorBorderColor #6b5336
    skinparam ActorBackgroundColor #efe4d6
    skinparam ParticipantBorderColor #6b5336
    skinparam ParticipantBackgroundColor #efe4d6
    skinparam SequenceLifeLineBorderColor #b59d82
